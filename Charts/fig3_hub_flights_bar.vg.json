{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "width": 500,
    "height": 400,
    "title": { "text": "" },
    "data": {
        "url": "Data/location_data_cleaned.csv"
    },
    "transform": [
        {"filter": "(datum.Flights > 15000)"}

    ],
    "mark": {"type": "bar"},
    "encoding": {
        "x": { "field": "Flights", "type": "quantitative", "title": "Number of Flights"},
